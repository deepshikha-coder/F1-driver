{
    "version": 2,
    "builds": [
        {
            "src": "dist/src/server.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "rewrites": [
        { "source": "/(.*)", "destination": "dist/src/server.js" }
    ],
    "headers": [
        {
          "source": "/dist/src/server.js",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }
    ]
}